{
  "name": "@linkiez/tinyerpv2-sdk",
  "version": "1.0.5",
  "description": "SDK for TinyERP v2 API",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc && node scripts/add-js-extensions.js && node esbuild.config.js",
    "dev": "ts-node --esm src/index.ts",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "clean": "rm -rf dist",
    "prepublishOnly": "yarn clean && yarn build",
    "prettier": "prettier --cache --write .",
    "prettier:check": "prettier --check .",
    "prepare": "husky",
    "semantic-release": "semantic-release"
  },
  "keywords": [
    "tinyerp",
    "sdk",
    "api",
    "v2",
    "typescript",
    "esm"
  ],
  "author": "linkiez",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/linkiez/TinyERPv2-SDK.git"
  },
  "homepage": "https://github.com/linkiez/TinyERPv2-SDK#readme",
  "bugs": {
    "url": "https://github.com/linkiez/TinyERPv2-SDK/issues"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "yarn@4.12.0",
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/exec": "^7.1.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^12.0.2",
    "@semantic-release/npm": "^13.1.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.2",
    "esbuild": "^0.25.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "semantic-release": "^25.0.2",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "axios": "^1.13.2"
  }
}
